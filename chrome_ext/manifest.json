{
  "manifest_version": 3,
  "name": "Time Tracker Extension",
  "version": "1.0",
  "description": "Track time spent on different websites",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}